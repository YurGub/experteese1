!!!
%html
  %head
    %meta{"http-equiv" => "X-UA-Compatible", content: ",IE=edge,chrome=1"}
    %title
      = content_for?(:title) ? yield(:title) : "Experteese"
    %meta{name: "viewport", content: "initial-scale=1, text/html; charset=utf-8,user-scalable=no,maximum-scale=1,width=device-width" }
    %meta{name: "description", content: "This is an Expert tool. You may leave your expert opinion to selected theme. Выбери тему и оставь свое экспертное мнение по ней."}/
    %meta{name: "keywords", content: "This is where you put all of the keywords you use in your site"}/
    = csrf_meta_tags

    %link{ :rel => "stylesheet", :href => "//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css"}
    / %link{:href => "https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css", :rel => "stylesheet"}

    = stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => 'reload'

    %script{:src => "//code.jquery.com/jquery-1.12.0.min.js"}
    %script{:src => "//code.jquery.com/jquery-migrate-1.2.1.min.js"}
    = javascript_include_tag "application"
    -#%script{:src => "//code.jquery.com/jquery-1.12.0.min.js"}
    -#%script{:src => "//code.jquery.com/jquery-migrate-1.2.1.min.js"}
    -#= javascript_include_tag "http://ajax.aspnetcdn.com/ajax/jquery.validate/1.9/jquery.validate.min.js"
    -#= javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"
  %body
    -#src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js">
    -#%script{:src => "http://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"}
    -#%script{:src => "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery.min.js"}
    -#<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.min.js"></script>
    -#%script{:src => "jquery-1.9.1.min.js", :type => "text/javascript"}

    -#= render 'ui/header'
    -#= render 'ui/flash'
    -#.container
    = yield
      -#= yield :debugger
    -#= render 'ui/footer'

